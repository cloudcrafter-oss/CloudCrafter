{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"globalEnv": ["CLOUDCRAFTER_AXIOS_BACKEND_BASEURL"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true,
			"inputs": ["$TURBO_DEFAULT$", ".env"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"outputs": []
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"//#format-and-lint": {},
		"//#format-and-lint:fix": {
			"cache": false
		}
	}
}
