{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "paths": {
      "@ui/*": [
        "../../packages/ui/src/*"
      ],
      "@/*": [
        "./*"
      ],
      "auth": [
        "./auth"
      ]
    }
  },
  "include": [
    "vite.config.ts"
  ]
}
